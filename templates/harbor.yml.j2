hostname: {{ harbor_fqdn | default(ansible_nodename) }}
http:
  port: {{ harbor_http_port }}
https:
  port: {{ harbor_https_port }}
  certificate: /etc/ssl/certs/{{ demo_crt_cert_file }}
  private_key: /etc/ssl/private/{{ demo_crt_key_file }}
harbor_admin_password: {{ harbor_admin_passwd | default('rack1234') }}
database:
  password: {{ harbor_db_passwd | default('rack1234') }}
  max_idle_conns: 100
  max_open_conns: 900
  conn_max_lifetime: 5m
  conn_max_idle_time: 0
data_volume: {{ harbor_base_dir }}
trivy:
  ignore_unfixed: false
  skip_update: false
  skip_java_db_update: false
  offline_scan: false
  security_check: vuln
  insecure: false
  timeout: 5m0s
jobservice:
  max_job_workers: 10
  max_job_duration_hours: 24
  job_loggers:
    - STD_OUTPUT
    - FILE
  logger_sweeper_duration: 1 #days
notification:
  webhook_job_max_retry: 3
  webhook_job_http_client_timeout: 3 #seconds
log:
  level: info
  local:
    rotate_count: 50
    rotate_size: 200M
    location: {{ harbor_base_dir }}/log/harbor
_version: {{ harbor_version | replace('v', '') }}
proxy:
  http_proxy:
  https_proxy:
  no_proxy:
  components:
    - core
    - jobservice
    - trivy
upload_purging:
  enabled: true
  age: 168h
  interval: 24h
  dryrun: false
cache:
  enabled: false
  expire_hours: 24
